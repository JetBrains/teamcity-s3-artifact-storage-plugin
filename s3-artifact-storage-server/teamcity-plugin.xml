<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>s3-artifact-storage</name>
    <display-name>Amazon S3 Artifact Storage</display-name>
    <version>@Version@</version>
    <description>Allows storing build artifacts in an Amazon S3 bucket</description>
    <vendor>
      <name>JetBrains</name>
      <url>http://www.jetbrains.com</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>
